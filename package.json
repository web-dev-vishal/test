{
  "name": "globalfi-ultra",
  "version": "1.0.0",
  "description": "High-performance financial orchestration engine",
  "type": "module",
  "main": "src/server.js",
  "engines": {
    "node": ">=18.0.0",
    "vscode": "^1.22.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:integration": "node test-integration.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "docker:build": "docker build -t globalfi-ultra .",
    "docker:run": "docker-compose up",
    "docker:stop": "docker-compose down",
    "redis:monitor": "node src/tools/redisMonitor.js --interactive",
    "redis:keys": "node src/tools/redisMonitor.js --list-keys",
    "redis:watch": "node src/tools/redisMonitor.js --watch",
    "redis:stats": "node src/tools/redisMonitor.js --stats",
    "redis:export": "node src/tools/redisMonitor.js --export ./cache-export.json",
    "redis:health": "node src/tools/redisMonitor.js --health",
    "ai:demo": "node examples/test-ai-features.js",
    "ai:integrate": "node examples/integrate-ai.js"
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "big.js": "^6.2.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.4",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "groq-sdk": "^0.37.0",
    "helmet": "^7.1.0",
    "inquirer": "^13.2.2",
    "ioredis": "^5.3.2",
    "mongoose": "^8.1.1",
    "ora": "^9.3.0",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "fintech",
    "financial-data",
    "orchestration",
    "real-time",
    "socket.io"
  ],
  "author": "Vishal Sanam",
  "license": "MIT"
}
