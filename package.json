{
  "name": "globalfi-ultra",
  "version": "1.0.0",
  "description": "High-performance financial orchestration engine",
  "type": "module",
  "main": "src/server.js",
  "engines": {
    "node": ">=18.0.0",
    "vscode": "^1.22.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:integration": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "docker:build": "docker build -t globalfi-ultra .",
    "docker:run": "docker-compose up",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "big.js": "^6.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "mongoose": "^8.1.1",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "fintech",
    "financial-data",
    "orchestration",
    "real-time",
    "socket.io"
  ],
  "author": "",
  "license": "MIT"
}
